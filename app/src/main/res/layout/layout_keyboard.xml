        app:layout_constraintStart_toEndOf="@id/layoutKeyboardACTE"
        app:layout_constraintTop_toTopOf="@id/layoutKeyboardACT9" />

    <Button
        android:id="@+id/layoutKeyboardACTDel"
        style="@style/Keyboard"
        android:text="@string/keyboardActDel"
        app:layout_constraintEnd_toStartOf="@id/layoutKeyboardACTClear"
        app:layout_constraintStart_toEndOf="@id/layoutKeyboardACTF"
        app:layout_constraintTop_toTopOf="@id/layoutKeyboardACT9" />

    <Button
        android:id="@+id/layoutKeyboardACTClear"
        style="@style/Keyboard"
        android:text="@string/keyboardActClear"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/layoutKeyboardACTDel"
        app:layout_constraintTop_toTopOf="@id/layoutKeyboardACT9" />
</merge>