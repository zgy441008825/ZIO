        app:layout_constraintTop_toTopOf="@id/layoutKeyboardACT0" />

    <Button
        android:id="@+id/layoutKeyboardACT8"
        style="@style/Keyboard"
        android:text="@string/keyboardAct8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/layoutKeyboardACT7"
        app:layout_constraintTop_toTopOf="@id/layoutKeyboardACT0" />


    <Button
        android:id="@+id/layoutKeyboardACT9"
        style="@style/Keyboard"
        android:text="@string/keyboardAct9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/layoutKeyboardACTA"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/layoutKeyboardACT0" />

    <Button
        android:id="@+id/layoutKeyboardACTA"
