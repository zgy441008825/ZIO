        app:layout_constraintTop_toTopOf="@id/layoutKeyboardACT0" />

    <Button
        android:id="@+id/layoutKeyboardACT3"
        style="@style/Keyboard"
        android:text="@string/keyboardAct3"
        app:layout_constraintEnd_toStartOf="@id/layoutKeyboardACT4"
        app:layout_constraintStart_toEndOf="@id/layoutKeyboardACT2"
        app:layout_constraintTop_toTopOf="@id/layoutKeyboardACT0" />

    <Button
        android:id="@+id/layoutKeyboardACT4"
        style="@style/Keyboard"
        android:text="@string/keyboardAct4"
        app:layout_constraintEnd_toStartOf="@id/layoutKeyboardACT5"
        app:layout_constraintStart_toEndOf="@id/layoutKeyboardACT3"
        app:layout_constraintTop_toTopOf="@id/layoutKeyboardACT0" />

    <Button
        android:id="@+id/layoutKeyboardACT5"
        style="@style/Keyboard"
        android:text="@string/keyboardAct5"
        app:layout_constraintEnd_toStartOf="@id/layoutKeyboardACT6"
        app:layout_constraintStart_toEndOf="@id/layoutKeyboardACT4"
        app:layout_constraintTop_toTopOf="@id/layoutKeyboardACT0" />

    <Button
        android:id="@+id/layoutKeyboardACT6"
        style="@style/Keyboard"
        android:text="@string/keyboardAct6"
