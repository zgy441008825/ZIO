        android:text="@string/keyboardAct0"
        app:layout_constraintBottom_toTopOf="@id/layoutKeyboardACT9"
        app:layout_constraintEnd_toStartOf="@id/layoutKeyboardACT1"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/layoutKeyboardACT1"
