<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="WatermarkView">
        <attr name="markViewColor" format="color" />
        <attr name="markViewTextSize" format="dimension" />
        <attr name="markViewText" format="string" />
        <attr name="markViewAngle" format="float" />
    </declare-styleable>

    <declare-styleable name="CircleProgressView">
        <attr name="centerColor" format="color" />
        <attr name="progressRingSize" format="float" />
        <attr name="progressBgColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="proTextColor" format="color" />
        <attr name="proTextSize" format="dimension" />
        <attr name="proValue" format="float" />
        <attr name="progressMax" format="integer" />
        <attr name="infoTextColor" format="color" />
        <attr name="infoTextSize" format="dimension" />
        <attr name="infoTextString" format="string" />
        <attr name="progressColorStart" format="color" />
        <attr name="progressColorEnd" format="color" />
    </declare-styleable>
    <declare-styleable name="LabelTextView">
        <attr name="labelString" format="string" />
        <attr name="labelPlace" format="boolean" />
        <attr name="labelColor" format="color" />
    </declare-styleable>
    <declare-styleable name="CustomFontTextView">
        <attr name="fontType" format="string"/>
    </declare-styleable>
</resources>